{
  "name": "sht-web",
  "version": "0.2.1",
  "description": "SaaS Prototype for a Volunteer Hours Management Application",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "8.4.0"
  },
  "scripts": {
    "start": "node ./build/server.js",
    "start:dev": "webpack-dev-server",
    "client:dev": "webpack-dashboard -t 'trails-web' -- webpack-dev-server",
    "client:watch": "webpack",
    "client:build": "npm run clean && cross-env NODE_ENV=\"production\" webpack",
    "client:preview": "npm run clean && cross-env NODE_ENV=\"production\" webpack-dev-server",
    "server:watch": "cross-env SERVER_RENDER=\"true\" webpack --config webpack.config.server.js",
    "server:restart": "nodemon ./build/server.js",
    "server:build": "cross-env SERVER_RENDER=\"true\" cross-env NODE_ENV=\"production\" webpack --config webpack.config.server.js",
    "server:dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"webpack-server,watch-server\" --kill-others \"npm run server:watch\" \"npm run server:restart\"",
    "universal:dev": "concurrently --prefix \"[{name}]\" --names \"webpack-server,webpack-client,watch-server\" --kill-others \"npm run server:watch\" \"npm run client:watch\" \"npm run server:restart\"",
    "universal:build": "npm run client:build && npm run server:build",
    "production:build": "npm run client:build && npm run server:build; gcloud app deploy",
    "clean": "rm -rf ./build",
    "lint-hook": "eslint ./source/js ./webpack ./*.js -f table --ext .js --ext .jsx",
    "lint": "npm run lint-hook --silent || true",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
